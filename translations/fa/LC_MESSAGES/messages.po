msgstr "این پیام توهین آمیز است."

msgid "button_why_reporting_2_text"
msgstr "این پیام نامربوط است."

msgid "button_why_reporting_3_text"
msgstr ":x:نه، نظرم تغییر کرد."

msgid "report_comment_text"
msgstr "یک نظر اضافه کنید."

